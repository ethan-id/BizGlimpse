{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "next"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "quotes": [2, "single", { "avoidEscape": true }],
      "no-unused-vars": "off"
    }
  }
  